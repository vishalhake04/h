<script type="text/javascript">

    $('#check-alert-inquiry').hide()
    $(document).ready(function(){
        $('#check-alert').hide()

        $('#pr-tab').click(function(){
            var element=document.getElementById('tab-content-link')
             $('.tab-content').css('height','800')
        });
        if(window.location.href.indexOf("contact_us") !== -1){
            var tab_id = 'tab-2';
            $('.help-cont').removeClass('current');
            $('.help-tab').removeClass('current');

            $(this).addClass('current');
            $("#"+tab_id).addClass('current');

            $('html, body').animate({
                scrollTop: $('#tab-2').offset().top
            },'slow');

        }
        $('#cr-tab').click(function(){
            var element=document.getElementById('tab-content-link')
            $('.tab-content').css('height','auto')
        });

        $('.help-cont').click(function(){
            var tab_id = $(this).attr('data-tab');
            $('.help-cont').removeClass('current');
            $('.help-tab').removeClass('current');

            $(this).addClass('current');
            $("#"+tab_id).addClass('current');

        });


        $("a").click(function(){
            var myelement = $(this).attr("data-href")
            $(myelement).slideToggle("slow");
            $(".toggle:visible").not(myelement).hide();

        });
