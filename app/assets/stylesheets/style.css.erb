/* photo gallery Grid */
.gal-container {
    padding: 12px;
}
.gal-item {
    overflow: hidden;
    padding: 2px;
    margin-bottom:10px;
}
.gal-item .box {
    height:329px;
    width:329px;
    overflow: hidden;
    -webkit-border-radius: 52px;
    -moz-border-radius: 52px;
    border-radius: 52px;
    margin-right: 10px;
    border:1px solid #ddd;
}
.box img {
    height: 100%;
    width: 100%;
    object-fit: cover;
    -o-object-fit: cover;
}
.gal-item a:focus {
    outline: none;
}
.gal-item a:after {
    content: "\e003";
    font-family: 'Glyphicons Halflings';
    opacity: 0;
    background-color: rgba(0, 0, 0, 0.75);
    position: absolute;
    right: 3px;
    left: 3px;
    top: 3px;
    bottom: 3px;
    text-align: center;
    line-height: 350px;
    font-size: 30px;
    color: #fff;
    -webkit-transition: all 0.5s ease-in-out 0s;
    -moz-transition: all 0.5s ease-in-out 0s;
    transition: all 0.5s ease-in-out 0s;
}
